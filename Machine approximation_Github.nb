Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"U", "(", "r", ")"}], " ", "=", " ", 
   RowBox[{
    RowBox[{
     SubscriptBox["a", "0"], 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[Alpha]"}], " ", 
       SuperscriptBox["r", "2"]}]]}], "+", " ", 
    RowBox[{
     SubscriptBox["a", "1"], 
