    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"a0", " ", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[Alpha]"}], " ", 
          RowBox[{"r", "^", "2"}]}], "]"}]}], "+", 
       RowBox[{"a1", " ", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[Alpha]"}], " ", "\[Beta]", " ", 
          RowBox[{"r", "^", "2"}]}], "]"}]}], "+", 
       RowBox[{"a2", " ", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[Alpha]"}], " ", 
