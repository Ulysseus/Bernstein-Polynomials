Cell[BoxData[
 RowBox[{
  RowBox[{"gaussG", "[", 
   RowBox[{"J_", ",", "r_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[Alpha]", "=", 
