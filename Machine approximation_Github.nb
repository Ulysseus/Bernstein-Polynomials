 FormBox[
  RowBox[{
   SubsuperscriptBox["\[Integral]", "0", "1"], 
   RowBox[{
    SuperscriptBox["y", 
     RowBox[{"1", "-", "1"}]], 
    SuperscriptBox["y", 
     RowBox[{"4", "-", "1"}]], 
    SuperscriptBox["y", 
     RowBox[{"n", "-", "m"}]], 
    RowBox[{"V", "(", "y", ")"}], 
    RowBox[{"\[DifferentialD]", "y"}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
