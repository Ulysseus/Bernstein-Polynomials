          RowBox[{"r", "^", "2"}]}], "]"}]}], "+", 
       RowBox[{"a4", " ", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[Alpha]"}], " ", 
          RowBox[{"\[Beta]", " ", "^", "4"}], 
          RowBox[{"r", "^", "2"}]}], "]"}]}]}], ")"}], "+", 
     RowBox[{"J", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"J", "+", "1"}], ")"}], "/", 
       RowBox[{"r", "^", "2"}]}]}]}]}], "]"}]}]], "Input",
