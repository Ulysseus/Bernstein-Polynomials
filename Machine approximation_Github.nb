      RowBox[{"\[Beta]", "=", 
       RowBox[{"179", "/", "100"}]}], ",", 
      RowBox[{"a0", "=", 
       RowBox[{
        RowBox[{"-", "915059"}], "/", "100000000"}]}], ",", 
      RowBox[{"a1", "=", 
       RowBox[{
        RowBox[{"-", "11127505"}], "/", "10000000"}]}], ",", 
      RowBox[{"a2", "=", 
       RowBox[{"323325687", "/", "100000000"}]}], ",", 
      RowBox[{"a3", "=", 
       RowBox[{"224888398", "/", "100000000"}]}], ",", 
      RowBox[{"a4", "=", 
