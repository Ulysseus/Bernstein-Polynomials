Cell[CellGroupData[{

Cell["\<\
Approximation by Bernstein Polynomials
\
\>", "Section",
 CellChangeTimes->{{3.695127056382204*^9, 3.695127072093371*^9}, 
   3.695466071070119*^9},
 CellID->1667670620],

Cell[TextData[{
 "The ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["n", "th"], TraditionalForm]]],
 " Bernstein polynomial of a function f(x) is ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["defined", "5"], TraditionalForm]]],
 " as f[x] = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"k", "=", "0"}], "n"], 
    RowBox[{
     RowBox[{"f", "[", 
      FractionBox["k", "n"], "]"}], " ", 
     RowBox[{"Binomial", "[", 
      RowBox[{"n", ",", " ", "k"}], "]"}], " ", 
     SuperscriptBox[
      RowBox[{
       SuperscriptBox["x", "k"], "(", 
       RowBox[{"1", "-", "x"}], ")"}], 
      RowBox[{"n", "-", "k"}]]}]}], TraditionalForm]]],
 " . Bernstein polynomials have the advantage that convergence is uniform and \
not pointwise. To check whether this results in a better approximation to the \
potential energy for this problem, than the one achieved using Jacobi \
polynomials, the following  function is defined:"
}], "Text",
 CellChangeTimes->{{3.69546607977409*^9, 3.695466302406292*^9}, {
  3.695466356008005*^9, 3.695466408345046*^9}, {3.695466459720694*^9, 
  3.695466467263453*^9}, {3.695618029231284*^9, 3.695618034854875*^9}, {
  3.6983822001578093`*^9, 3.698382232189118*^9}, {3.698382275205102*^9, 
  3.698382287917027*^9}, {3.700825883234789*^9, 3.700825935770447*^9}, {
  3.700894619524826*^9, 3.700894620109005*^9}, {3.701177564716874*^9, 
  3.701177565482359*^9}, {3.7011776039944887`*^9, 3.701177693362775*^9}},
 CellID->748909857],

Cell[BoxData[
 RowBox[{
  RowBox[{"approxBernstein", "[", "n_", "]"}], ":=", 
  RowBox[{
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"k", "=", "0"}], "n"], 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"gaussG", "[", 
       RowBox[{"0", ",", "r"}], "]"}], "/.", 
      RowBox[{"r", "\[Rule]", 
       RowBox[{"-", 
        RowBox[{"Log", "[", 
         FractionBox["k", "n"], "]"}]}]}]}], ")"}], " ", 
    RowBox[{"Binomial", "[", 
     RowBox[{"n", ",", "k"}], "]"}], " ", 
    SuperscriptBox["y", "k"], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "-", "y"}], ")"}], 
     RowBox[{"n", "-", "k"}]]}]}]}]], "Code",
 CellChangeTimes->{{3.695466449932377*^9, 3.695466498216168*^9}, {
   3.695618077396716*^9, 3.695618108420889*^9}, 3.698127703758794*^9, {
   3.7196710391104603`*^9, 3.719671051198398*^9}},
 CellID->1072084445,
 ExpressionUUID -> "df4e7ee9-8abd-4beb-b03a-fbb4a1bb2d4d"],

