      RowBox[{
       RowBox[{"-", "\[Alpha]"}], " ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"\[Beta]", "  ", "r"}], ")"}], "2"]}]]}], "+", " ", 
    RowBox[{
     SubscriptBox["a", "3"], 
     SuperscriptBox["\[ExponentialE]", 
