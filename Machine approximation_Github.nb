Cell[TextData[{
 "\nNow V(y) is approximated as a polynomial in y. To achieve this, use will \
be made of the Jacobi orthogonal ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["polynomials", "4"], TraditionalForm]]],
 ".  The polynomials are defined by the following function:\n "
}], "Text",
 CellChangeTimes->{{3.695017444230761*^9, 3.6950176448788013`*^9}, 
   3.697954764699141*^9, {3.699269439001936*^9, 3.699269445347385*^9}, {
   3.7008942902873487`*^9, 3.700894290903192*^9}, {3.701174202086203*^9, 
   3.7011742500200377`*^9}},
 CellID->29979050],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", 
    RowBox[{"n_", ",", "p_", ",", "q_", ",", "y_"}], "]"}], ":=", 
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{"q", "+", "n", "-", "1"}], ")"}], "!"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"p", "+", 
        RowBox[{"2", " ", "n"}], "-", "1"}], ")"}], "!"}]], 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"m", "=", "0"}], "n"], 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "m"], 
      RowBox[{"Binomial", "[", 
       RowBox[{"n", ",", "m"}], "]"}], "   ", 
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"p", "+", 
          RowBox[{"2", " ", "n"}], "-", "m", "-", "1"}], ")"}], "!"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"q", "+", "n", "-", "m", "-", "1"}], ")"}], "!"}]], 
      SuperscriptBox["y", 
       RowBox[{"n", "-", "m"}]]}]}]}]}], " ", "\[LineSeparator]"}]], "Input",
 CellChangeTimes->{{3.6950176640810547`*^9, 3.6950176674387083`*^9}, {
   3.695018965622398*^9, 3.6950189763580217`*^9}, {3.695463948191986*^9, 
   3.69546395282268*^9}, 3.697954840274123*^9, 3.6979552296723623`*^9, 
   3.6981252173526173`*^9, {3.698125379256559*^9, 3.6981253934803257`*^9}},
 CellID->995399162,
 ExpressionUUID -> "ebcb80bc-3ebd-4dcc-b8a8-83b929becad4"],

Cell["\<\
This defines the Jacobi orthogonal polynomials over [0,1]. The normalization \
constant is given by the following function:
\
\>", "Text",
 CellChangeTimes->{{3.695017684071556*^9, 3.695017733254744*^9}, 
   3.695018149196357*^9, {3.7011742568921747`*^9, 3.7011742581240387`*^9}, {
   3.70160029224636*^9, 3.701600293654374*^9}},
 CellID->745033851],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{
      SuperscriptBox["y", 
       RowBox[{"q", "-", "1"}]], 
      SuperscriptBox["y", 
       RowBox[{"p", "-", "q"}]], 
      RowBox[{"g", "[", 
       RowBox[{"n", ",", "p", ",", "q", ",", "y"}], "]"}], 
      RowBox[{"g", "[", 
       RowBox[{"m", ",", "p", ",", "q", ",", "y"}], "]"}]}]}], " ", "=", " ", 
    
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"n", "!"}], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"n", "+", "q", "-", "1"}], ")"}], "!"}], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"n", "+", "p", "-", "1"}], ")"}], "!"}], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"n", "+", "p", "-", "q"}], ")"}], "!"}]}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", "n"}], "+", "p"}], ")"}], 
        RowBox[{"(", 
         SuperscriptBox[
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "n"}], "+", "p", "-", "1"}], ")"}], "!"}], 
          "2"]}]}]], "  ", "n"}], "=", 
     RowBox[{
      RowBox[{"m", " ", "and", " ", "0", " ", "if", " ", "n"}], " ", 
      "\[NotEqual]", " ", "m"}]}]}], "\[IndentingNewLine]"}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{
  3.6950178093082857`*^9, {3.695017861514862*^9, 3.695017983966535*^9}, {
   3.69501801771931*^9, 3.6950181356783457`*^9}, 3.695019221990445*^9, {
   3.697954873633831*^9, 3.697954895896649*^9}},
 CellID->800307706],

Cell[BoxData[
 RowBox[{
  RowBox[{"hn", "[", 
   RowBox[{"n_", ",", "p_", ",", "q_"}], "]"}], ":=", 
  FractionBox[
   RowBox[{
    RowBox[{"n", "!"}], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", "+", "q", "-", "1"}], ")"}], "!"}], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", "+", "p", "-", "1"}], ")"}], "!"}], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", "+", "p", "-", "q"}], ")"}], "!"}]}], 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", "n"}], "+", "p"}], ")"}], 
    SuperscriptBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "n"}], "+", "p", "-", "1"}], ")"}], "!"}], 
     "2"]}]]}]], "Input",
 CellChangeTimes->{3.697954904501677*^9},
 CellID->1438226662],

Cell["\<\
To expand V(y) in terms of Jacobi polynomials would require the evaluation of \
the following integral:\
\>", "Text",
 CellChangeTimes->{{3.695018176814996*^9, 3.695018213758226*^9}, 
   3.6950183064780893`*^9, {3.7011742676600237`*^9, 3.701174268788104*^9}},
 CellID->1188659632],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["a", 
    RowBox[{"n", " "}]], "=", 
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "1"], 
    RowBox[{
     SuperscriptBox["y", 
      RowBox[{"q", "-", "1"}]], 
     SuperscriptBox["y", 
      RowBox[{"p", "-", "q"}]], 
     RowBox[{"g", "[", 
      RowBox[{"n", ",", "p", ",", "q", ",", "y"}], "]"}], 
     RowBox[{"V", "(", "y", ")"}], 
     RowBox[{"\[DifferentialD]", "y"}]}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.6950184620884438`*^9, 3.695018497990141*^9}, {
  3.6950185405919037`*^9, 3.695018588270094*^9}, {3.695019055350819*^9, 
  3.695019063061932*^9}},
 CellID->118591185],

Cell["The expansion is given by:", "Text",
 CellChangeTimes->{{3.69501909544667*^9, 3.6950191011498537`*^9}, 
   3.701174274684959*^9},
 CellID->1292399698],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "n"}], " ", "+", "p"}], ")"}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", "n"}], " ", "+", "p", " ", "+", "1"}], ")"}], 
        "!"}], ")"}], "2"]}], 
    RowBox[{
     RowBox[{"n", "!"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "+", "q", "-", "1"}], ")"}], "!"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "+", "p", "-", "1"}], ")"}], "!"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "+", "p", "-", "q"}], ")"}], "!"}]}]], 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"n", "=", "0"}], "\[Infinity]"], 
    RowBox[{
     SubscriptBox["a", "n"], 
     RowBox[{"g", "[", 
      RowBox[{"n", ",", "p", ",", "q", ",", "y"}], "]"}]}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.695019120665744*^9, 3.695019249294302*^9}},
 CellID->840768201],

Cell["\<\
The series is truncated and the polynomials expanded and subsequently reduced \
to give a polynomial in y.\
\>", "Text",
